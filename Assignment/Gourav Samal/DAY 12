// PERFECT NUMBERS//

start=1
end=5000
c=0
for i in range(start,end):
    c=0
    for j in range(1,i):
        if(i%j==0):
            c = c + j
    if(i==c):
        print(i,end='\n')



//STRING REVERSE//

str = 'Gandhi Institute For Technology'
rev = ''
for i in range(len(str)-1,-1,-1):
	rev = rev + str[i]
print(rev,end='')

