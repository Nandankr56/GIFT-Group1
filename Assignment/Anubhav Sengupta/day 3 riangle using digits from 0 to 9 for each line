package com.algo;

public class pattern_F {

	public static void main(String[] args) {
		int spaces = 35;
		int stars = 1;
		for(int i=0; i<9; i++) {
			for(int j=0; j<spaces; j++) System.out.print(" ");
			for(int j=0; j<stars; j++)  System.out.print(i);
			System.out.println();
			spaces--; stars+=2;
		}
	}
}
